{
  "globals"   : {
    "describe"   : false,
    "it"         : false,
    "before"     : false,
    "beforeEach" : false,
    "after"      : false,
    "afterEach"  : false
  },
  "asi": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "freeze": true,
  "indent": 2,
  "latedef": "nofunc",
  "laxcomma": true,
  "laxbreak": true,
  "node": true,
  "trailing": true,
  "strict": true,
  "unused": "vars",
  "undef": true,
  "-W116": true,
  "-W040": true,
  "-W098": true,
}
